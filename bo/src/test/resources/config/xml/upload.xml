<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<upload-base>upload</upload-base>
    <uri>http://localhost:8090/blog</uri>
    
    <uploads>
		<upload>
	        <type>image</type>
	        <name>图片</name>
	       	<extTypes>
	       		<value>gif</value>
	       		<value>jpg</value>
	       		<value>jpeg</value>
	       		<value>png</value>
	       		<value>bmp</value>
	       	</extTypes>
	        
	        <thirdPartys>
	            <third channel="YEE_PAY" code="ICBC" />
	            <third channel="CHINA_PAY" code="0102" />
	            <third channel="PING_AN" code="10001" />
	            <third channel="UM_PAY" code="ICBC" />
	            <third channel="BAO_FOO" code="ICBC" />
	             <third channel="HF_PAY" code="0102" />
	        </thirdPartys>
	    </upload>
	    <upload>
	        <type>flash</type>
	        <name>动画</name>
	        <extTypes>
	       		<value>swf</value>
	       		<value>flv</value>
	       	</extTypes>
	    </upload>
	    <upload>
	        <type>media</type>
	        <name>视频</name>
	        <extTypes>
	       		<value>swf</value>
	       		<value>flv</value>
	       		<value>mp3</value>
	       		<value>wav</value>
	       		<value>wma</value>
	       		<value>wmv</value>
	       		<value>mid</value>
	       		<value>avi</value>
	       		<value>mpg</value>
	       		<value>asf</value>
	       		<value>rm</value>
	       		<value>rmvb</value>
	       	</extTypes>
	    </upload>
	    <upload>
	        <type>file</type>
	        <name>文件</name>
	        <extTypes>
	       		<value>doc</value>
	       		<value>docx</value>
	       		<value>xls</value>
	       		<value>xlsx</value>
	       		<value>ppt</value>
	       		<value>htm</value>
	       		<value>html</value>
	       		<value>txt</value>
	       		<value>zip</value>
	       		<value>rar</value>
	       		<value>gz</value>
	       		<value>bz2</value>
	       	</extTypes>
	    </upload>
	</uploads>
	
</settings>
