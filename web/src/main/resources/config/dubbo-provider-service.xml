<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
    <dubbo:application name="${dubbo.provider.application.name}"/>
    
	<dubbo:registry protocol="${dubbo.provider.registry.protocol}" address="${dubbo.provider.registry.address}"/>
	
	<dubbo:monitor protocol="${dubbo.monitor.protocol}"/>
	
    <dubbo:protocol name="${dubbo.provider.protocol.name}" port="${dubbo.provider.protocol.port}"/>
	<!-- 
    <dubbo:protocol name="http" port="8889"/>
    <dubbo:protocol name="hessian" port="8890"/>
    <dubbo:protocol name="webservice" port="8892"/>
	 -->
    
	<bean id="openQueryService" class="com.yueny.cms.dubbo.impl.OpenDubboQueryServiceImpl"/>
	<bean id="systemQueryService" class="com.yueny.cms.dubbo.impl.SystemDubboQueryServiceImpl"/>
	<bean id="systemParameterQueryService" class="com.yueny.cms.dubbo.impl.SystemParameterQueryServiceImpl"/>
	
	<dubbo:service ref="systemQueryService" interface="com.yueny.cms.api.service.ISystemQueryService"/>
	<dubbo:service ref="openQueryService" interface="com.yueny.cms.api.service.IOpenQueryService"/>
	<dubbo:service ref="systemParameterQueryService" interface="com.yueny.cms.api.service.ISystemParameterQueryService" />
	
</beans>