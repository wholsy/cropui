<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<!-- 扫描注解包路径，多个包用逗号分隔，不填pacakge表示扫描当前ApplicationContext中所有的类  -->
	<dubbo:annotation package="com.yueny.blog.service.integration"/>
	
	<dubbo:reference id="configureQueryService" check="false"
		interface="com.yueny.cms.api.service.IConfigureQueryService"/>
	<dubbo:reference id="systemQueryService" check="false"
		interface="com.yueny.cms.api.service.ISystemQueryService"/>
 	<dubbo:reference id="openQueryService" check="false"
		interface="com.yueny.cms.api.service.IOpenQueryService"/>
    
</beans>